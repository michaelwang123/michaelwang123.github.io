  "framework": "hugo",
  "installCommand": "curl -OL https://golang.org/dl/go1.22.1.linux-amd64.tar.gz && rm -rf /usr/local/go && tar -C /usr/local -xzf go1.22.1.linux-amd64.tar.gz"
} 